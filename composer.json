{
    "name": "gaaab/pdf-to-image",
    "description": "Convert a pdf to an image",
    "keywords": [
        "spatie",
        "pdf-to-image",
        "pdf",
        "convert",
        "image"
    ],
    "homepage": "https://github.com/spatie/pdf-to-image",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Badiola",
            "email": "web.gabriel.badiola@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Gaaab/Org_Heigl_Ghostscript.git"
        }
    ],
    "require": {
        "php" : "^7.0",
        "gaaab/ghostscript": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.2",
        "spatie/temporary-directory": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PdfToImage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PdfToImage\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
